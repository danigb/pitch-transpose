  "description": "Simple and fast pitch transposition",
  "main": "index.js",
  "scripts": {
    "test": "vows --spec test/*.js",
    "docs": "docme README.md",
    "dist": "browserify index.js > dist/index.jsÂ && browserify transpose.js | uglifyjs > dist/transpose.min.mjs && ls -all dist/*",
